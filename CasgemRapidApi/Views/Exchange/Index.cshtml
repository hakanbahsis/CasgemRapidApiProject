@model List<CasgemRapidApi.Models.ExchangeViewModel.Exchange_Rates>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    int count = 0;
}

<h1>Index</h1>

<table class="table table-bordered">
    <tr>
        <td>#</td>
        <td>Döviz</td>
        <td>Miktar</td>
    </tr>
    @foreach (var item in Model)
    {
        count++;
        <tr>
            <td>@count</td>
            <td>@item.currency</td>
            <td>@item.exchange_rate_buy</td>

        </tr>
    }
</table>

